{
  "name": "webanyone/mautic-ecommerce-bundle",
  "description": "MauticEcommerceBundle",
  "type": "mautic-plugin",
  "keywords": [
    "mautic",
    "plugin",
    "integration",
    "ecommerce"
  ],
  "extra": {
    "install-directory-name": "MauticEcommerceBundle",
    "branch-alias": {
      "dev-main": "0.0.x-dev"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MauticPlugin\\MauticEcommerceBundle\\": ""
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4.0 <8.1",
    "mautic/core-lib": "^4.0",
    "symfony/serializer": ">=4.4",
    "guzzlehttp/oauth-subscriber": "^0.6.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/dennisameling/FOSOAuthServerBundle.git"
    }
  ],
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "phpunit/phpunit": "9.5.x-dev",
    "phpspec/prophecy-phpunit": "2.0.x-dev",
    "jangregor/phpstan-prophecy": "dev-master",
    "phpstan/extension-installer": "1.2.x-dev",
    "phpstan/phpstan-doctrine": "1.3.x-dev",
    "phpstan/phpstan": "1.8.x-dev"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "phpstan/extension-installer": true
    }
  }
}
